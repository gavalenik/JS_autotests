{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "plugins": [
        "wdio"
    ],
    "extends": [ //"eslint:recommended", "airbnb-base",
        "standard",
        "plugin:wdio/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest" //"sourceType": "module"
    },
    "rules": { //"class-methods-use-this": [0]
        "no-undef": ["off"], // "warn" // "error"
        "no-eval": ["warn", {"allowIndirect": true}] // default is false
    }
}
